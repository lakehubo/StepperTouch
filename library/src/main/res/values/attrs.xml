<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StepperTouch">
        <!-- Styleable dimensions -->
        <attr name="stepperTextSize" format="dimension" />

        <attr name="stepperAllowNegative" format="boolean" />
        <attr name="stepperAllowPositive" format="boolean" />
        <attr name="stepperOrientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="stepperSingle" format="boolean">
        </attr>
    </declare-styleable>

</resources>
