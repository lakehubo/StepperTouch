<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StepperTouch">
        <!-- Styleable dimensions -->
        <attr name="stepperTextSize" format="dimension" />

        <attr name="stepperAllowNegative" format="boolean" />
        <attr name="stepperAllowPositive" format="boolean" />
        <attr name="stepperOrientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="stepperSingle" format="boolean">
        </attr>
    </declare-styleable>
    <declare-styleable name="WindSpeedGroupView">
        <attr name="groupDividerWidth" format="integer"/>
        <attr name="groupSize">
            <enum name="two" value="0" />
            <enum name="three" value="1" />
            <enum name="four" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ShaderBtnView">
        <attr name="imgSrc" format="reference"/>
        <attr name="imgSrc_press" format="reference"/>
        <attr name="imgWidth" format="dimension"/>
        <attr name="imgHeight" format="dimension"/>
        <attr name="text" format="string"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="solid" format="color"/>
        <attr name="solid_press" format="color"/>
        <attr name="radius" format="dimension"/>
        <attr name="shadowColor" format="color"/>
        <attr name="shadowDx" format="float"/>
        <attr name="shadowDy" format="float"/>
        <attr name="shadowTop" format="dimension"/>
        <attr name="shadowLeft" format="dimension"/>
        <attr name="shadowRight" format="dimension"/>
        <attr name="shadowBottom" format="dimension"/>
        <attr name="corner" format="dimension"/>
        <attr name="btnType">
            <enum name="oval" value="0" />
            <enum name="rectangle" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TouchImageGridLineView">
        <attr name="zoom_enabled" format="boolean" />
    </declare-styleable>
</resources>
